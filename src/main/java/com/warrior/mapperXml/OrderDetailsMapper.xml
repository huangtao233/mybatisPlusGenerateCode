<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.warrior.mapper.OrderDetailsMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.warrior.entity.OrderDetails">
		<id column="DETAIL_ID" property="detailId" />
		<result column="ORDER_ID" property="orderId" />
		<result column="SHOE_ID" property="shoeId" />
		<result column="COUNT" property="count" />
		<result column="PRICE" property="price" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        DETAIL_ID AS detailId, ORDER_ID AS orderId, SHOE_ID AS shoeId, COUNT AS count, PRICE AS price
    </sql>

</mapper>
